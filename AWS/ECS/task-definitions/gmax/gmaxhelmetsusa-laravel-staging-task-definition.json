{
  "family": "gmaxhelmetsusa-laravel-staging-task-definition",
  "volumes": [
    {
      "host": {
        "sourcePath": "/home/ec2-user/web-settings/staging/gmaxhelmetsusa-laravel/.env"
      },
      "name": "GmaxHelmetsUsaVolume"
    }
  ],
  "containerDefinitions": [
    {
      "essential": true,
      "image": "wpsinc/gmaxhelmetsusa-laravel:%REVISION%",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 100,
      "mountPoints": [
        {
          "sourceVolume": "GmaxHelmetsUsaVolume",
          "containerPath": "/var/www/html/.env"
        }
      ],
      "name": "gmaxhelmetsusa-laravel"
    },
    {
      "essential": true,
      "image": "wpsinc/gmaxhelmetsusa-nginx:latest",
      "links": [
        "gmaxhelmetsusa-php-fpm"
      ],
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 100,
      "name": "gmaxhelmetsusa-nginx",
      "portMappings": [
        {
          "hostPort": 50011,
          "containerPort": 80
        }
      ],
      "volumesFrom": [
        {
          "sourceContainer": "gmaxhelmetsusa-laravel"
        }
      ]
    },
    {
      "essential": true,
      "image": "wpsinc/gmaxhelmetsusa-php-fpm:latest",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 200,
      "name": "gmaxhelmetsusa-php-fpm",
      "volumesFrom": [
        {
          "sourceContainer": "gmaxhelmetsusa-laravel"
        }
      ]
    }
  ]
}