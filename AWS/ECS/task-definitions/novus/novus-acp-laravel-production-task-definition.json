{
  "family": "novus-acp-production-task-definition",
  "volumes": [
    {
      "host": {
        "sourcePath": "/home/ec2-user/web-settings/production/novus-acp-laravel/.env"
      },
      "name": "NovusAcpVolume"
    }
  ],
  "containerDefinitions": [
    {
      "essential": true,
      "image": "wpsinc/novus-acp-laravel:%REVISION%",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 100,
      "mountPoints": [
        {
          "sourceVolume": "NovusAcpVolume",
          "containerPath": "/var/www/html/.env"
        }
      ],
      "name": "novus-acp-laravel"
    },
    {
      "essential": true,
      "image": "wpsinc/novus-acp-nginx:latest",
      "links": [
        "novus-acp-php-fpm"
      ],
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 200,
      "name": "novus-acp-nginx",
      "portMappings": [
        {
          "hostPort": 60005,
          "containerPort": 80
        }
      ],
      "volumesFrom": [
        {
          "sourceContainer": "novus-acp-laravel"
        }
      ]
    },
    {
      "essential": true,
      "image": "wpsinc/novus-acp-php-fpm:latest",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 300,
      "name": "novus-acp-php-fpm",
      "volumesFrom": [
        {
          "sourceContainer": "novus-acp-laravel"
        }
      ]
    }
  ]
}