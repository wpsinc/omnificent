{
  "family": "novus-api-cron-production-task-definition",
  "volumes": [
    {
      "host": {
        "sourcePath": "/home/ec2-user/web-settings/production/novus-api-laravel/.env"
      },
      "name": "VolumeNovusApi"
    }
  ],
  "containerDefinitions": [
    {
      "essential": true,
      "image": "wpsinc/novus-api-cron:%REVISION%",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 100,
      "name": "novus-api-cron",
      "volumesFrom": [
        {
          "sourceContainer": "novus-api-laravel"
        }
      ]
    },
    {
      "essential": true,
      "image": "wpsinc/novus-api-laravel:latest",
      "logConfiguration": {
        "logDriver": "json-file",
        "options": {
          "max-size": "50m"
        }
      },
      "memory": 100,
      "mountPoints": [
        {
          "sourceVolume": "VolumeNovusApi",
          "containerPath": "/var/www/html/.env"
        }
      ],
      "name": "novus-api-laravel"
    }
  ]
}